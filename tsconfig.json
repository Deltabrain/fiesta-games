{
	"extends": "expo/tsconfig.base",
	"module": "esnext",
	"target": "es2017",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@assets/*": ["./src/assets/*"],
			"@components/*": ["./src/components/*"],
			"@constants/*": ["./src/constants/*"],
			"@themed/*": ["./src/components/themed/*"],
			"@hooks/*": ["./src/hooks/*"],
			"@lib/*": ["./src/lib/*"],
			"@util/*": ["./src/util/*"],
			"@types": ["./src/lib/types.ts"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
