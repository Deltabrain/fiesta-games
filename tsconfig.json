{
	"extends": "expo/tsconfig.base",
	"module": "esnext",
	"target": "es2017",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"@/src/*": ["./src/*"],
			"@/components/*": ["./src/components/*"],
			"@/hooks/*": ["./src/hooks/*"],
			"@/lib/*": ["./src/lib/*"],
			"@/types": ["./src/lib/types.ts"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
