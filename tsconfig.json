{
	"extends": "expo/tsconfig.base",
	"module": "esnext",
	"target": "es2017",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"],
			"@/src/*": ["./src/*"],
			"@/components/*": ["./components/*"],
			"@/hooks/*": ["./hooks/*"],
			"@/db/*": ["./db/*"],
			"@/types": ["./util/types.ts"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
