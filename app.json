{
	"expo": {
		"name": "Fiesta",
		"slug": "Fiesta-Trinkspiele",
		"owner": "deltabrain",
		"version": "0.2.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#151718"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "io.deltabrain.fiesta",
			"googleServicesFile": "./google/google-services.json",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#151718"
			}
		},
		"androidStatusBar": {
			"hidden": true
		},
		"androidNavigationBar": {
			"backgroundColor": "#151718"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"@react-native-firebase/app",
			"@react-native-firebase/auth"
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "7f14d39f-e934-4e03-865c-45a205720657"
			}
		}
	}
}
